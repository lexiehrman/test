  {% comment %} Webfonts - Note: This theme uses mission gothic as @fontface {% endcomment %}
  <link href='http://fonts.googleapis.com/css?family=Bevan' rel='stylesheet' type='text/css'>

  {% comment %} Stylesheets {% endcomment %}
  {{ 'style.min.css' | asset_url | stylesheet_tag }}

  <!--[if lt IE 9]>
  {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
  <![endif]-->

  {% comment %} jQuery upfront so it can be used in templates {% endcomment %}
  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}

  {% comment %} Scripts required up {% endcomment %}
  <script>
    function imgLoaded(img){
      var $img = $(img);
      $img.addClass('loaded').siblings('.css-loader').remove();
    };
    function imgLoadedLoader(img){
      var $img = $(img);
      $img.addClass('loaded');
    };
  </script>

  {% comment %} Header hook for plugins {% endcomment%}
  {{ content_for_header }}

  {% comment %}
    Conditional Scripts -- Remember to move these into each customer template instead when using pjax
  {% endcomment%}
  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}
</head>

<body>

  {% include 'header' %}

  <main class="main" role="main" id="main">

    {% if template == 'list-collections' %}
      {% include 'collection-listing' %}
    {% else %}
      {{ content_for_layout }}
    {% endif %}

    {% include 'footer' %}

  </main>

  {% include 'ajax-cart-template' %}

  {% comment %} Application{% endcomment%}
  {{ 'main.min.js' | asset_url | script_tag }}

  <script>
  jQuery(function($) {
    var moneyFormat = {{ shop.money_format | json }};
    app.ajax( moneyFormat );
  });
  </script>


</body>
</html>
