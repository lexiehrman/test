<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html class="no-js" itemscope itemtype="http://schema.org/{% case template %}{% when 'product' %}Product{% when 'blog' %}Blog{% else %}LocalBusiness{% endcase %}" xmlns:fb="http://ogp.me/ns/fb#">
<!--<![endif]-->
<head>

  {% comment %} Basic Needs {% endcomment %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  {% comment %} DNS Prefetching {% endcomment %}
  <link rel="dns-prefetch" href="//cdn.myshopify.com/">
  <link rel="dns-prefetch" href="//s3-us-west-1.amazonaws.com">
  <link rel="dns-prefetch" href="//fonts.googleapis.com">
  <link rel="dns-prefetch" href="//ajax.googleapis.com">

  {% comment %} Meta {% endcomment %}
  {% if template == "product" %}
    <meta name="keywords" content="{% for tag in product.tags %}{{tag}}, {% endfor %}" />
    <meta itemprop="description" name="description" content="{{ product.description | strip_html | truncate: 150 }}" />
    <meta itemprop="image" content="{{ product.featured_image | product_img_url: 'compact' }}">
    <meta itemprop="currency" content="{{ shop.currency }}">
    <meta itemprop="seller" content="{{ shop.name }}">
    <meta itemprop="availability" content="{% if product.available %}in_stock{% else %}out_of_stock{% endif %}">
  {% elsif template == "article" %}
    <meta name="description" itemprop="description" content="{{ article.content | strip_html | truncate: 150 }}" />
  {% else %}
    <meta name="description" itemprop="description" content="{{ shop.description }}">
    <meta name="keywords" content="{{ product.description | strip_html | truncate: 150 }}">
  {% endif %}

  {% comment %} Social Meta {% endcomment %}
  {% include 'fb-open-graph-tags' %}
  {% include 'twitter-card' %}

  {% comment %} Favicon {% endcomment %}
  <link rel="shortcut icon" href="{{ 'favicon.ico' | asset_url }}" type="image/x-icon">
  <link rel="icon" href="{{ 'favicon.ico' | asset_url }}" type="image/x-icon">

  {% comment %} Helpers {% endcomment %}
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="apple-mobile-web-app-capable" content="yes" />
  <link rel="canonical" href="{{ canonical_url }}" />
  <link rel="alternate" type="application/rss+xml" title="{{ shop.name }} : Products" href="{{ shop.url }}/collections/all.atom" />
  <link rel="sitemap" type="application/xml" title="Sitemap" href="/sitemap.xml" />

  {% comment %} Custom Page Titles {% endcomment %}
  <title>
    {{shop.name}}
    {% if template != "index" %} | {% endif %}
    {% if template == 'list-collections' %}
      SHOP
    {% elsif template contains "page" %}
      {{ page.title }}
    {% elsif template contains "blog" %}
      {{ blog.title }}
    {% elsif template contains "article" %}
      {{ article.title }}
    {% elsif template contains "product" %}
      {{ product.title }}
    {% elsif template contains "collection" %}
      {{collection.title }}
    {% elsif template contains "cart" %}
      Cart
    {% elsif template contains "404" %}
      Not Found
    {% elsif template contains "search" %}
      Search
    {% elsif template contains "login" %}
      Sign In
    {% elsif template contains "register" %}
      Register
    {% elsif template contains "reset" %}
      Reset Password
    {% elsif template contains "order" %}
      Order Detail
    {% elsif template contains "addresses" %}
      My Addresses
    {% elsif template contains "activate" %}
      Activate Account
    {% elsif template contains "account" %}
      My Account
    {% endif %}
  </title>

